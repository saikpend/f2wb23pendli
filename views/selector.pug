extends layout

block content
  h1= title
  - let cars_img_names = ["lamborghini.jpg","lamborghini1.jpg","rolls royce.jpg","rolls-royce-ghost.jpg","audi.jpg"]
  - for(let i=0;i<3;i++){
      - let img = Math.floor(Math.random()*5);
      - let item = cars_img_names[img];
      img(src=`/images/${item}` alt=`${item}` style='height:300px; width:300px') 
  - }